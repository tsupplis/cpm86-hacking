#!/bin/sh
prog=`basename $0|sed -e s'/aztec34_//g'`.exe
export EMU2_DRIVE_D="`dirname $0`/../share/aztec34"
mkdir -p "$TMPDIR/AZTEC/TEMP"
export EMU2_DRIVE_E="$TMPDIR/AZTEC"
export EMU2_PROGNAME="d:\\bin\\$prog"
emu2 "$EMU2_DRIVE_D/bin/$prog" "$@" -- PATH=D:\\BIN CLIB=D:\\LIB\\ INCLUDE=D:\\INCLUDE \
  CCTEMP="E:\\TEMP"
